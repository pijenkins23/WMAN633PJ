issn = {0275-5947},
journal = {North American Journal of Fisheries Management},
month = {feb},
number = {1},
pages = {213--221},
publisher = {Wiley},
title = {{Contribution of Tiger Muskellunge to the Sport Fishery of a Small, Centrarchid-Dominated Impoundment}},
volume = {12},
year = {1992}
