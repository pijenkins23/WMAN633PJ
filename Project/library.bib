journal = {North American Journal of Fisheries Management},
number = {2},
pages = {539--544},
title = {{Effects of Implantation Method and Temperature on Mortality and Loss of Simulated Transmitters in Hybrid Striped Bass}},
volume = {129},
year = {2000}
}
