@article{Margenau1992,
abstract = {Stocking hatchery-reared muskellunge Esox masquinongy is important to Wisconsin's muskellunge management program. Typically, large (8-12-in) fingerlings are stocked in fall; however, these fish have poor short-term (30-60-d) survival. The author compared success of fish stocked as fall fingerlings (FF) and spring yearlings (SY). Overwinter survival of FF averaged 19{\%}. Mortality was highest from stocking through late fall, then declined over winter. In three lakes stocked with both FF and SY, SY survived better (19{\%}) than FF (4{\%}) to age 18 months. Cost analysis based on survival over winter revealed no consistent economic advantage in stocking larger, more expensive fingerlings within the 8-12-in range unless precise information is known about potential predators. Cost comparisons indicated SY were one to four times more cost-effective than FF to age 18 months. Stocking SY should provide a better return to the fishery per hatchery dollar than stocking FF},
author = {Margenau, Terry L.},
doi = {10.1577/1548-8675(1992)012<0484:saceos>2.3.co;2},
issn = {0275-5947},
journal = {North American Journal of Fisheries Management},
