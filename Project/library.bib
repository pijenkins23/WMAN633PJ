keywords = {Biotelemetry,Conservation,Data interpretation,Fishery management,Uncertainty},
month = {jun},
number = {2},
pages = {369--400},
publisher = {Springer International Publishing},
title = {{Conducting and interpreting fish telemetry studies: considerations for researchers and resource managers}},
volume = {29},
year = {2019}
}
