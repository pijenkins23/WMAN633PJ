journal = {Transactions of the American Fisheries Society},
number = {3},
pages = {402--410},
title = {{Comparison of Food Consumption, Growth, and Metabolism among Muskellunge: An Investigation of Population Differentiation}},
volume = {125},
year = {1996}
}
